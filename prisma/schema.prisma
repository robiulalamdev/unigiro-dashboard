generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Define the Company model
model Financial {
  id                Int    @id @default(autoincrement())
  exchange          String
  company           String
  stocksPrices      Json
  financialOverview Json

  @@map("financial")
}
